#!/bin/sh
case "$(printf "mpv\\ninvidio.us\\nlibreoffice\\nsignal\\nvirt-manager" | dmenu)" in
	"mpv") mpv --player-operation-mode=pseudo-gui ;;
	"invidio.us") firefox --new-window https://invidio.us ;;
	"libreoffice") libreoffice ;;
	"signal") signal-desktop ;;
	"virt-manager") virt-manager ;;
esac
