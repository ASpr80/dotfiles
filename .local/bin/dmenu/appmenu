#!/bin/sh
case "$(printf "mpv\\nfirefox\\ninvidio.us\\nprivatebrowser\\nnetflix\\nlibreoffice\\nretroarch\\nvirt-manager" | dmenu)" in
	"mpv") mpv --player-operation-mode=pseudo-gui ;;
	"firefox") firefox ;;
	"invidio.us") qutebrowser --target=window https://invidio.us ;;
	"privatebrowser") qutebrowser ":open -p";;
	"netflix") qtwebflix ;;
	"libreoffice") libreoffice ;;
	"retroarch") retroarch ;;
	"virt-manager") virt-manager ;;
esac
